using System;

class Program 
{
    static void Main()
    {
        Console.WriteLine("Assign an int value to N:");
        int n = int.Parse(Console.ReadLine());

        PrintSquarePattern(n);
    }

    static void PrintSquarePattern(int n)
    {
        for(int row = 0; row<n;row++){
            for(int col = 0; col<n;col++)
            {
                if(row>=col)

                    Console.Write('*');
            }
            Console.WriteLine("");
        }



    }
}
