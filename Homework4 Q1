using System;

class Program
{
    static void Main()
    {
        int a = 3;
        int b = 5;
        int largestNumber = GetLargestNumber(a, b);
        Console.WriteLine("a=3; b=5");
     Console.WriteLine ("The largest number is: " + largestNumber);
    }
    static int GetLargestNumber(int a, int b)
    {
        if (a > b)
        {
            return a;
        }
        else
        {
            return b;
        }
    }
}
