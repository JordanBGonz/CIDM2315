using System;

class Program 
{
    static void Main()
    {
        Console.WriteLine("Assign an int value to N:");
        int n = int.Parse(Console.ReadLine());

        PrintSquarePattern(n);
    }

    static void PrintSquarePattern(int n)
    {
        for (int i = 0; i < n; i++)
        {
            for (int j = 0; j < n; j++)
            {
                Console.Write("# ");
            }
            Console.WriteLine();
        }
    }
}
